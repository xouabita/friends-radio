tada($transformation)
  @keyframes tada
    from
      transform $transformation scale3d(1, 1, 1)

    10%, 20%
      transform $transformation scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg)

    30%, 50%, 70%, 90%
      transform $transformation scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg)

    40%, 60%, 80%
      transform: $transformation scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg)

    to
      transform $transformation scale3d(1, 1, 1)
  animation-name tada
